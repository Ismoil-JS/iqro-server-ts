{
  "compilerOptions": {
    "module": "CommonJS",
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ESNext",
    "outDir": "dist",
    "baseUrl": "src",
    "rootDir": "src",
    "paths": {
      "@config": [
        "./config"
      ],
      "@prisma": [
        "./prisma"
      ],
      "@module": [
        "./module"
      ]
    },
    "skipLibCheck": true,
    "noImplicitAny": false,
  }
}
